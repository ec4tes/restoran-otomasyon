<Application x:Class="RestoranOtomasyon.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:RestoranOtomasyon"
             xmlns:converters="clr-namespace:RestoranOtomasyon.Converters">
    <Application.Resources>
        <converters:StringEqualsVisibilityConverter x:Key="StringEqualsVisibility"/>
        <converters:NullToVisibilityConverter x:Key="NullToVisibility"/>
        <converters:BoolToVisibilityConverter x:Key="BoolToVisibility"/>
        <converters:InverseBoolConverter x:Key="InverseBool"/>
    </Application.Resources>
</Application>
